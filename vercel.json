{
    "builds": [
        {
            "src": "NOTEBOOK/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "NOTEBOOK/frontend/build/static/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "NOTEBOOK/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "NOTEBOOK/frontend/build/index.html"
        }
    ]
}